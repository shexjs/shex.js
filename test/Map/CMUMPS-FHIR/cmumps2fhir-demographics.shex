      # };
      # fhir:Patient.contact.address { 
        # fhir:Address.line       { fhir:value xsd:string? %Map:{ dem:estreet_address_1 %} };
        # fhir:Address.city       { fhir:value xsd:string? %Map:{ dem:ecity %} };
        # fhir:Address.postalCode { fhir:value xsd:string? %Map:{ dem:ezip %} };
        # fhir:Address.type       { fhir:value xsd:string %Map:{ dem:constAddrPostal %} };
      # };
      # fhir:Patient.contact.telecom { 
        # fhir:ContactPoint.system { fhir:value xsd:string %Map:{ dem:constCmumpsEphone %} };
        # fhir:ContactPoint.value { fhir:value xsd:string? %Map:{ dem:ephone %} };
      # };
    # };
  
    # fhir:Patient.communication @<Patient.communication>*;  # A list of Languages which may be
