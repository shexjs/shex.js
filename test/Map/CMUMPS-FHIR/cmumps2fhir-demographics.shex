      # fhir:Patient.contact.relationship {
        # fhir:Element.id { fhir:valid xsd:string %Map:{ dem:erelationship_id %} };
        # fhir:CodeableConcept.coding {
          # fhir:Coding.system  { fhir:value xsd:string %Map:{ dem:constCmumpsSys %} };
          # fhir:Coding.version { fhir:value xsd:string %Map:{ dem:constCmumpsVersion %} };
          # fhir:Coding.display { fhir:value xsd:string %Map:{ dem:erelationship_label %} };
