{
  "name": "@shexjs/webapp",
  "version": "1.0.0-alpha.6",
  "description": "Shape Expressions command line interface",
  "author": {
    "name": "Eric Prud'hommeaux",
    "email": "eric@w3.org",
    "url": "http://www.w3.org/People/Eric/"
  },
  "keywords": [
    "shex",
    "shape expressions",
    "rdf",
    "query",
    "parser"
  ],
  "license": "MIT",
  "main": "./shex-webapp.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "@shexjs/core": "file:../shex-core",
    "@shexjs/api": "file:../shex-api",
    "@shexjs/parser": "file:../shex-parser",
    "n3": "^1.6.2",
    "shape-map": "file:../shape-map"
  },
  "scripts": {
    "make": "(cd test/ && make test)",
    "browser-webpack": "npx webpack --config webpack.config.js --mode production --profile --json > browser/compilation-stats.json ; npm run n3-browserify",
    "n3-browserify": "mkdir -p node_modules/n3/browser ; npx browserify -s N3 -o node_modules/n3/browser/n3-browserify.js node_modules/n3/lib/index.js ; npx uglifyjs node_modules/n3/browser/n3-browserify.js -o node_modules/n3/browser/n3-browserify.min.js -c",
    "test": "mocha -C -R dot"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shexSpec/grammar.git"
  },
  "bugs": {
    "url": "https://github.com/shexSpec/grammar/issues"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "devDependencies": {
    "terser-webpack-plugin": "^4.1.0",
    "uglify-es": "^3.3.9",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  }
}
